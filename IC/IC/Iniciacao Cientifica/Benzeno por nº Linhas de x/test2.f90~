PROGRAM	test2
IMPLICIT NONE
INTEGER::r,n,k,i
DOUBLE PRECISION:: pi, rad, y, x, w, z, l,a
l=1.41D0
z=0.0D0
a=k*2.0D0
PRINT*, 'Insira a quantidade de linhas'
READ*, k
OPEN(UNIT=1,FILE='test2.xyz')
WRITE(1,*) a
WRITE(1,*)
DO i=1,k
IF (MOD(i,2).eq.0) THEN
IF (r==3) CYCLE
IF (r==4) CYCLE
ELSE IF (MOD(i,2).eq.1) THEN
IF (r==2) CYCLE
IF (r==5) CYCLE
DO r=2,5
	w=r*60.0D0-30.0D0
	pi=dacos(-1.0D0)
	rad= pi*w*(2.0D0/360.0D0)
	x=(i-1)*l*dcos(rad)-l*sqrt(3.0D0)
	y=l*dsin(rad)
END IF
WRITE(1,*) 'c',x,y,z
END DO
END DO
 CLOSE(UNIT=1)
END PROGRAM
